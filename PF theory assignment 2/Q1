#include<stdio.h>
int main(){
	int arr[5];
	int value= 9999,value2=9998;
	for(int i=0;i<5;i++){
		printf("element- %d: ",i);
		scanf("%d",&arr[i]);
		if(value>arr[i]){
			value2=value;
			value=arr[i];
	    }
		else if(arr[i]>value && arr[i]<value2){
			value2=arr[i];
	    }
		
		
	}
	
	printf("The second smallest element in the array is: %d",value2);
return 0;
}
